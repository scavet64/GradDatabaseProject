@model IEnumerable<Kinabalu.Models.Views.ProductShipmentView>

@{
    ViewData["Title"] = "ProductShipment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Product Shipment Dates</h2>

<p>
    <a asp-action="Index">Go Back</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Customer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Product)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShipmentDate)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Customer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Product)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ShipmentDate)
            </td>
        </tr>
    }
    </tbody>
</table>
