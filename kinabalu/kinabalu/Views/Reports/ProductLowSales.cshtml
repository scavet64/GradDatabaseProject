@model IEnumerable<Kinabalu.Models.Views.ProductLowSalesView>

@{
    ViewData["Title"] = "ProductLowSales";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Product Low Sales</h2>

<p>
    <a asp-action="Index">Go Back</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Source)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sales)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Source)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sales)
            </td>
        </tr>
}
    </tbody>
</table>
